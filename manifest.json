{
  "manifest_version": 2,
  "name": "Fetch Fiddler",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "modifyFetch.js"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ]
}
